gcc -Wall -pedantic -c server.c
gcc -Wall -pedantic -c chess_svc.c
gcc -Wall -pedantic -c chess_xdr.c
gcc -o chessServer server.o chess_svc.o chess_xdr.o

gcc -Wall -pedantic -c client.c
gcc -Wall -pedantic -c chess_clnt.c
gcc -Wall -pedantic -c chess_xdr.c
gcc -o chessClient client.o chess_clnt.o chess_xdr.o

